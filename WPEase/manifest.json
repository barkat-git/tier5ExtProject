{
	"name": "WPEase",
	"description": "Let you know About Wordpress sites",
	
	"version": "1.0",
	
	"manifest_version": 2,
	"minimum_chrome_version": "37",
	"permissions": ["activeTab", "http://*/*", "https://*/*", "https://api.themesinfo.com/*"],
	"background": {
		"persistent": false,
		"scripts": ["js/background.js"]
	},
	"browser_action": {
		"default_icon": "icons/icon-none.png",
		"default_popup": "html/popup.html"
	},
	"icons" : {
        "16":"icons/icon16.png",
		"48" : "icons/icon48.png",
		"128" : "icons/icon128.png"
	},
	"key": ""
}